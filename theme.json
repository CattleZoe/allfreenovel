{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "duotone": [],
      "customDuotone": false,
      "defaultDuotone": false,
      "gradients": [],
      "customGradient": false,
      "defaultGradients": false
    },
    "typography": {
      "dropCap": true,
      "lineHeight": false,
      "textTransform": false,
      "letterSpacing": false,
      "customFontSize": false,
      "fontSizes": [
        {
          "size": "var(--fs-xxs)",
          "name": "Extra Small",
          "shortName": "XXS",
          "slug": "extra-small"
        },
        {
          "size": "var(--fs-xs)",
          "name": "Small",
          "shortName": "XS",
          "slug": "small"
        },
        {
          "size": "var(--fs-s)",
          "name": "Normal",
          "shortName": "S",
          "slug": "normal"
        },
        {
          "size": "var(--fs-m)",
          "name": "Medium",
          "shortName": "M",
          "slug": "medium"
        },
        {
          "size": "var(--fs-l)",
          "name": "Large",
          "shortName": "L",
          "slug": "large"
        },
        {
          "size": "var(--fs-xl)",
          "name": "Extra Large",
          "shortName": "XL",
          "slug": "extra-large"
        },
        {
          "size": "var(--fs-xxl)",
          "name": "Huge",
          "shortName": "XXL",
          "slug": "huge"
        },
        {
          "size": "var(--fs-xxxl)",
          "name": "Gigantic",
          "shortName": "XXXL",
          "slug": "gigantic"
        }
      ]
    },
    "blocks": {
      "core/image": {
        "lightbox": {
          "enabled": false,
          "allowEditing": false
        }
      }
    }
  }
}
